    <!-- Intro Header -->
    <header class="intro">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2"><br>
                        <h2>"we cannot solve our problems with the same thinking we used when we created them."</h2>
                        <p class="intro-text">Albert Einstein</p>
                        <a href="#about" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- About Section -->
    <section id="about" class="container content-section text-center">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2>About Big Brainpan</h2>
                <p>Workflow automation often entails streamlining of unstructured tasks involving people, processes and products. Big Brainpan is collection of
		ideas to integrate technology into your day-to-day workflows. </p>
            </div>
        </div>
    </section>

    <!-- sms2spark Section -->
    <% if (user) { %>
    <section id="sms2spark" class="content-section text-center">
        <div class="download-section">
            <div class="container">
                <div class="col-lg-8 col-lg-offset-2">
                    <h2>Test drive the sms-2-spark BOT</h2>
                    <h5>Register a mobile number and take the SMS-2-SPARK BOT for a spin!</h5>
                    <form>
                    <div class="input-group">
                    </div>

                    <div>
                      <table align="center">
                        <tr>
                          <!-- <td><span class="input-group-addon"><i class="fa fa-mobile fa-fw" aria-hidden="true"></i></span></td> -->
                          <td><input id = "cellphone" name="mobile_number" class="form-control" type="text" placeholder="(204) 111 1121"></td>
                        </tr>
                      </table>

                    </div>
                    <div class="checkbox">
                      <label><input type="checkbox">Send custom message</label>
                    </div>
                    <button type="submit" class="btn btn-default"><i class="fa fa-paper-plane fa-fw" aria-hidden="true"></i> Submit</button>
                  </form>

                    <p>Register for <a href="http://www.ciscospark.com">Cisco Spark</a> and take the SMS-2-SPARK BOT for a spin!</p>
		    <a href="https://exmachina-sparkspigot.c9users.io:8081/login" class="btn btn-default btn-lg">Login</a>
			<br><br>
                    <a href="https://github.com/zoneix/c2k_msg" class="btn btn-default btn-sm">Get the open source BOT code</a>
                </div>
            </div>
        </div>
    </section>
    <% } %>

    <!-- Contact Section -->
    <section id="contact" class="container content-section text-center">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2>Contact Us</h2>
                <p>Feel free to email us to provide some feedback on our ideas, suggest workflow problems to solve, creative concepts, or to just say hello!</p>
                <p><a href="mailto:feedback@bigbrainpan.com">feedback@bigbrainpan.com</a>
                </p>

                <ul class="list-inline banner-social-buttons">
                    <li>
                        <a href="https://twitter.com/zoneix"><i class="fa fa-twitter-square fa-fw fa-3x" aria-hidden="true"></i></a>
                    </li>
                    <li>
                        <a href="https://github.com/zoneix"><i class="fa fa-github fa-fw fa-3x" aria-hidden="true"></i></a>
                    </li>
                </ul>

            </div>
        </div>
    </section>

